<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Temp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#64b5f6</item>
        <item name="colorPrimaryVariant">#000000</item>
        <item name="colorOnPrimary">#000000</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#fafafa</item>
        <item name="colorSecondaryVariant">#fafafa</item>
        <item name="colorOnSecondary">#fafafa</item>
        <item name="colorButtonNormal">@color/app_blue</item>
        <item name="buttonTint">@color/app_blue</item>
        <item name="drawableTint">@color/dark_theme_blue</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Temp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Temp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Temp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>